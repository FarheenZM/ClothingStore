<p class = "pageHeader">Accessories</p>

<div class = "container">
    <table>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Price</th>
        </tr>
        #foreach($accessory in $accessories)
        <td>
            <td>$accessory.getTitle()</td>
            <td>$accessory.getDescription()</td>
            <td>$accessory.getPrice()</td>
            <td></td><a href="/accessories/$accessory.getId()">Show</a></td>
        </tr>
        #end
    </table>
</div>



